{
  "extends": [
    "next",
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "dot-notation": "error",
    "no-floating-decimal": "error",
    "array-bracket-spacing": "error",
    "computed-property-spacing": ["error", "never"],
    "curly": "error",
    "no-lonely-if": "error",
    "no-prototype-builtins": "off",
    "react/no-deprecated": "error",
    "arrow-body-style": ["error", "as-needed"],
    "no-unused-vars": "warn"
  }
}
